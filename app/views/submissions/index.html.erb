<h1>Listing submissions</h1>

<table>
  <tr>
    <th>Problem</th>
    <th>User</th>
    <th>Result</th>
    <th>Time</th>
    <th>Used memory</th>
    <th>Used time</th>
    <th>Lang</th>
    <th>Visible group</th>
    <th>Path</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.problem_id %></td>
    <td><%= submission.user_id %></td>
    <td><%= submission.result %></td>
    <td><%= submission.time %></td>
    <td><%= submission.used_memory %></td>
    <td><%= submission.used_time %></td>
    <td><%= submission.lang %></td>
    <td><%= submission.visible_group %></td>
    <td><%= submission.path %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submission', new_submission_path %>
