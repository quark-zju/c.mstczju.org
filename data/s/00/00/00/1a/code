#include <stdio.h>
#include <string.h>

int main() {
    int T;
    int map[101][101];
    int w, h, ww, hh, n;
    int iw, ih, ix, iy, rp;
    int i, j, p, q;
    int curr, best;

    scanf("%d", &T);
    while (T--) {
        memset(map, 0, sizeof(map));
        scanf("%d%d%d%d%d", &w, &h, &ww, &hh, &n);
        for (i = 0; i < n; i++) {
            scanf("%d%d%d%d%d", &rp, &iw, &ih, &ix, &iy);
            for (p = 0; p < iw; p++)
                for (q = 0; q < ih; q++)
                    map[ix + p][iy + q] = rp;
        }
        best = 0x7fffffff;
        for (i = 0; i < w; i++) {
            if (i + ww > w) break;
            for (j = 0; j < h; j++) {
                if (j + hh > h) break;
                curr = 0;
                for (p = 0; p < ww; p++)
                    for (q = 0; q < hh; q++)
                        curr += map[p + i][q + j];
                if (curr < best)
                    best = curr;
            }
        }
        printf("%d%s\n", best, (best >= 30) ? " cerror BG!" : "");
    }
    return 0;
}

