#include <stdio.h>
#include <stdlib.h>
#include <string.h>
void str2low(char* a)
{
	while(*a != '\0') {
		*a = *a | 0x20;
		++a;
	}
}

int main(int argc, char const* argv[])
{
	int l, n;
	int i;
	int count;
	char format[27];
	char* sample;
	char* present;

	while(scanf("%d %d", &l, &n) != EOF) {
		if(l == 0 && n == 0)
			break;
		sample = (char*)malloc(n+1);
		present = (char*)malloc(n+1);
		count = 0;

		sprintf(format, "%s%d%s", "%", n, "s%*[^\n]s");
		scanf(format, sample);
		
		str2low(sample);
		for (i = 1; i < l; i++) {
			scanf(format, present);
			str2low(present);
			if(strcmp(present, sample) == 0)
				++count;
		}
		free(sample);
		free(present);
		printf("%d\n", (count == 0) ? -1:count);
	}
	return 0;
}

