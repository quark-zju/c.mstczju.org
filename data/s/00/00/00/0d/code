#include <stdio.h>
#include <string.h>

int c,sc,cid;

struct song{
    char date[30], id[30];
    int count;
} songs[200];

void insert(char date[], char id[]) {
    int i;
    for (i=0;i<sc;i++) {
        if (strcmp(songs[i].date, date) ==0 && strcmp(songs[i].id, id) ==0) {
            songs[i].count++;
            goto judge;
        }
    }
    strcpy(songs[sc].date, date);
    strcpy(songs[sc].id, id);
    songs[sc].count = 1;
    i = sc++;
judge:
    if ((songs[i].count > c) || (songs[i].count == c && i < cid)) {
        cid=i;
        c=songs[i].count;
    }
}

int main(int argc, char const* argv[])
{
    int ti, tc;
    scanf("%d", &tc);
    for (ti = 0; ti < tc; ti++) {
        int n, i;
        c = 0;
        sc = 0;
        char artist[30], date[30], id[30];
        scanf("%d", &n);
        for (i = 0; i < n; i++) {
            scanf(" %s %s %s %*[^\n]s", artist, date, id);
            if (strcmp(artist, "syz") == 0) insert(date, id);
        }
        printf("%s %s\n", songs[cid].date, songs[cid].id);
    }
    return 0;
}
