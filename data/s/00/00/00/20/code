#include <stdio.h>

 int main()
 {
 int i; int d;
 char a[10000000];
 for (i = 0; i < 10000000; i ++)
    a[i] = (char)i;
 for (i = 0; i < 1000000; ++i)
    a[i] = (char)(a[i+100] * a[i+200] + a[i+300]);
 d = 0;
 for (i = 0; i < 10000000; ++i)
    d += a[i];

 if (d == 0)
 return 0;
  else
    return 1;
  
 }